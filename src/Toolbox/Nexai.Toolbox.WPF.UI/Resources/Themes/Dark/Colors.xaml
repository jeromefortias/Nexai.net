<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PrimaryColor">#1E1E1E</Color>
    <Color x:Key="SecondaryColor">#252526</Color>
    <Color x:Key="ComplementColor">#414141</Color>
    <Color x:Key="SecondaryComplementColor">#565656</Color>

    <Color x:Key="HighContrastColor">#DDDDDD</Color>

    <Color x:Key="AccentColor">#FF519ABA</Color>
    <Color x:Key="DisabledAccentColor">#4C519ABA</Color>
    <Color x:Key="ContrastAccentColor">#FFDE6325</Color>

    <Color x:Key="UnFocusForegroundColor">#969696</Color>
    <Color x:Key="ForegroundColor">#FFFFFF</Color>
    <Color x:Key="DisabledForegroundColor">#595959</Color>

    <Color x:Key="ForegroundOnAccentColor">#FFFFFF</Color>
    <Color x:Key="ForegroundOnContrastAccentColor">#000000</Color>

    <!-- (100% FF) - (85% D8) -->
    <Color x:Key="HighlightMaskColor">#D8FFFFFF</Color>

    <!-- (60% 99) - (55% 8D) -->
    <Color x:Key="NormalMaskColor">#8DFFFFFF</Color>

    <!-- 30% -->
    <Color x:Key="DisabledMaskColor">#4CFFFFFF</Color>

    <Color x:Key="SuccessColor">#8AE28A</Color>
    <Color x:Key="ErrorColor">#FF3333</Color>
    <Color x:Key="WarningColor">#DFCA62</Color>

</ResourceDictionary>