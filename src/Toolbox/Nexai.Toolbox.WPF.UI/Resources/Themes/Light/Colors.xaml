<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="PrimaryColor">#FFFFFF</Color>
    <Color x:Key="SecondaryColor">#F3F3F3</Color>
    <Color x:Key="ComplementColor">#DDDDDD</Color>
    <Color x:Key="SecondaryComplementColor">#AAAAAA</Color>

    <Color x:Key="HighContrastColor">#252526</Color>

    <Color x:Key="AccentColor">#FF519ABA</Color>
    <Color x:Key="DisabledAccentColor">#4C519ABA</Color>
    <Color x:Key="ContrastAccentColor">#FFDE6325</Color>

    <Color x:Key="UnFocusForegroundColor">#565656</Color>
    <Color x:Key="ForegroundColor">#000000</Color>
    <Color x:Key="DisabledForegroundColor">#969696</Color>

    <Color x:Key="ForegroundOnAccentColor">#FFFFFF</Color>
    <Color x:Key="ForegroundOnContrastAccentColor">#000000</Color>

    <!-- 100% (85%) -->
    <Color x:Key="HighlightMaskColor">#FFFFFFFF</Color>

    <!-- 60% (55%) -->
    <Color x:Key="NormalMaskColor">#99FFFFFF</Color>

    <!-- 30% -->
    <Color x:Key="DisabledMaskColor">#4CFFFFFF</Color>

    <Color x:Key="SuccessColor">#8AE28A</Color>
    <Color x:Key="ErrorColor">#FF3333</Color>
    <Color x:Key="WarningColor">#DFCA62</Color>

</ResourceDictionary>